{
  "name": "rental-management-system",
  "version": "1.0.0",
  "description": "A comprehensive rental management system for managing quotations, orders, inventory, and invoicing",
  "private": true,
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "server": "node server/index.js",
    "server:dev": "nodemon server/index.js",
    "dev": "concurrently \"npm start\" \"npm run server:dev\"",
    "migrate": "node server/migrations/run.js",
    "seed": "node server/seeds/run.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "axios": "^1.6.2",
    "date-fns": "^3.0.0",
    "formik": "^2.4.5",
    "yup": "^1.3.3",
    "react-datepicker": "^4.24.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "recharts": "^2.10.3",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6"
  },
  "optionalDependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "multer": "^1.4.5-lts.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "mongoose": "^8.0.3",
    "nodemon": "^3.0.2",
    "razorpay": "^2.9.2",
    "stripe": "^14.9.0",
    "pdfkit": "^0.14.0",
    "exceljs": "^4.4.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "sharp": "^0.33.1",
    "cloudinary": "^1.41.1",
    "aws-sdk": "^2.1514.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "react/prop-types": "warn"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "rental",
    "management",
    "erp",
    "quotation",
    "inventory",
    "invoicing",
    "react",
    "nodejs",
    "express",
    "postgresql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/rental-management-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/rental-management-system/issues"
  },
  "homepage": "https://github.com/yourusername/rental-management-system#readme"
}